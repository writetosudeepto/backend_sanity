# Discord Webhook URL
# Get this from Discord: Server Settings > Integrations > Webhooks > Create Webhook
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# Sanity Webhook Secret
# Set this in Sanity Studio webhook configuration for security
SANITY_WEBHOOK_SECRET=your-secret-key

# Server Port (optional)
PORT=3001